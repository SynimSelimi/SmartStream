        <Literal xml:lang="fr">Site</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>Site</IRI>
        <Literal xml:lang="es">sede</Literal>
